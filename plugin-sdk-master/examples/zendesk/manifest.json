    "description": {
        "ja": "Zendesk is a beautifully simple customer service platform. The Zendesk for kintone plugin gives your team visibility into Zendesk tickets from within any of your kintone apps.",
        "en": "Zendesk is a beautifully simple customer service platform. The Zendesk for kintone plugin gives your team visibility into Zendesk tickets from within any of your kintone apps.",
        "zh": "Zendesk is a beautifully simple customer service platform. The Zendesk for kintone plugin gives your team visibility into Zendesk tickets from within any of your kintone apps."
    },
    "icon": "img/icon.png",
    "homepage_url": {
        "ja": "https://cybozudev.zendesk.com/hc/ja/articles/205831710",
        "en": "https://developer.kintone.io/hc/en-us/articles/209064437"
    },
    "desktop": {
        "js": ["https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js",
